{
    "name": "Youtube Video Speeder",
    "description": "Speed your videos",
    "version": "1.0",
    "manifest_version": 3,
    "permissions" : [
        "tabs", 
        "activeTab",
        "storage"
    ],
    "content_scripts": [
      {
        "matches": [
            "<all_urls>"
        ],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_icon" : "/images/speed.png",
      "default_popup": "popup.html",
      "default_title": "Popup"
  },
  "icons":{
      "16": "/images/speed16.png",
      "32": "/images/speed32.png",
      "64": "/images/speed64.png"
  }
}
  